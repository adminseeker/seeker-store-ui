<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <role
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                        emptyText="Select Signup role"
                        fieldDescription="Please Select Signup role"
                        fieldLabel="Signup role"
                        name="./role">
                        <items jcr:primaryType="nt:unstructured">
							<variation1
                                jcr:primaryType="nt:unstructured"
                                text="buyer"
                                value="buyer"/>
                            <variation2
                                jcr:primaryType="nt:unstructured"
                                text="seller"
                                value="seller"/>
                        </items>
                    </role>
                    <signupTitle
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Signup Component Title"
                        name="./signupTitle"/>
                    <firstNameLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="First Name Label"
                        name="./firstNameLabel"/>
                    <lastNameLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Last Name Label"
                        name="./lastNameLabel"/>
                    <emailAddressLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Email Address Label"
                        name="./emailAddressLabel"/>
                    <invalidEmailError
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Invalid Email Error"
                        name="./invalidEmailError"/>
                    <regesiteredEmailError
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Registered Email Error"
                        name="./regesiteredEmailError"/>
                    <passwordLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Password Label"
                        name="./passwordLabel"/>
                    <confirmPasswordLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Confirm Password Label"
                        name="./confirmPasswordLabel"/>
                    <minimumPasswordLength
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Minimum Password Length"
                        name="./minimumPasswordLength"/>
                    <invalidPasswordError
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Invalid Password Error"
                        name="./invalidPasswordError"/>
                    <passwordMismatchError
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Password Mismatch Error"
                        name="./passwordMismatchError"/>
                    <phoneLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Phone Label"
                        name="./phoneLabel"/>
                    <invalidPhoneError
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Invalid Phone Error"
                        name="./invalidPhoneError"/>
                    <submitButtonLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Submit Button  Label"
                        name="./submitButtonLabel"/>
                    <signinLinkLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Sign in Label"
                        name="./signinLinkLabel"/>
                    <signinLink
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldLabel="Sign in Link"
                        name="./signinLink"
                        rootPath="/content/seekerstore"/>
                    <dashboardLink
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldDescription="Page Link to land after Signup"
                        fieldLabel="Dashboard Link"
                        name="./dashboardLink"
                        rootPath="/content/seekerstore"/>
                    <genericError
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Generic Error"
                        name="./genericError"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>